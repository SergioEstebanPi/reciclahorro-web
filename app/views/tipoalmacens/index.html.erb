<p id="notice"><%= notice %></p>

<h1>Tipoalmacens</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th>Descripcion</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tipoalmacens.each do |tipoalmacen| %>
      <tr>
        <td><%= tipoalmacen.nombre %></td>
        <td><%= tipoalmacen.descripcion %></td>
        <td><%= link_to 'Show', tipoalmacen %></td>
        <td><%= link_to 'Edit', edit_tipoalmacen_path(tipoalmacen) %></td>
        <td><%= link_to 'Destroy', tipoalmacen, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tipoalmacen', new_tipoalmacen_path %>
