<p id="notice"><%= notice %></p>

<h1>Solicitud</h1>

<table>
  <thead>
    <tr>
      <th>Titulo</th>
      <th>Descripcion</th>
      <th>Fecha recoleccion</th>
      <th>Usuario</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @solicitud.each do |solicitud| %>
      <tr>
        <td><%= solicitud.titulo %></td>
        <td><%= solicitud.descripcion %></td>
        <td><%= solicitud.fecha_recoleccion %></td>
        <td><%= solicitud.usuario %></td>
        <td><%= link_to 'Show', solicitud %></td>
        <td><%= link_to 'Edit', edit_solicitud_path(solicitud) %></td>
        <td><%= link_to 'Destroy', solicitud, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Solicitud', new_solicitud_path %>
